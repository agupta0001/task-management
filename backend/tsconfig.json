{
  "extends": "../base.tsconfig.json",
  "compilerOptions": {
    "outDir": "build",
    "target": "ES6",
    "noImplicitAny": false,
    "strictFunctionTypes": false,
    "useUnknownInCatchVariables": false,
    "strictPropertyInitialization": false
  },
  "ts-node": {
    "files": true
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules", "build"]
}
